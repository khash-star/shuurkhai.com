RewriteRule ^report?$ report.php
RewriteRule ^customers?$ customers.php
RewriteRule ^orders?$ orders.php
RewriteRule ^order_cp72?$ order_cp72.php
RewriteRule ^tracks?$ tracks.php
RewriteRule ^cp72?$ cp72.php
RewriteRule ^mini?$ mini.php
RewriteRule ^container_cp72?$ container_cp72.php
RewriteRule ^container_item_print?$ container_item_print.php
RewriteRule ^boxes_preview?$ boxes_preview.php
RewriteRule ^combine_preview?$ combine_preview.php
